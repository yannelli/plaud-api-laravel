{
    "name": "yannelli/laravel-plaud",
    "description": "Unofficial Laravel package for the Plaud API - manage recordings, transcriptions, and summaries",
    "type": "library",
    "keywords": [
        "laravel",
        "plaud",
        "plaud-api",
        "recordings",
        "transcription",
        "audio"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Yannelli\\LaravelPlaud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yannelli\\LaravelPlaud\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yannelli\\LaravelPlaud\\PlaudServiceProvider"
            ],
            "aliases": {
                "Plaud": "Yannelli\\LaravelPlaud\\Facades\\Plaud"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
